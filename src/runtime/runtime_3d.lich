fungsi hitungJarak3D(pos1, pos2) {
    dx = pos1.x - pos2.x
    dy = pos1.y - pos2.y
    dz = pos1.z - pos2.z
    kembalikan akar(dx * dx + dy * dy + dz * dz)
}